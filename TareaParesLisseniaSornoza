HacerMemoriaMatriz <- function(x = matriZ()) {
   inversa <- NULL
   set <- function(y){
         x <<- y
         inversa <<- NULL
	} 
   get <- function() x 
   setInverse <- function(inverse) inversa <<- inverse
   getInverse <- function() inversa
   list (set = set,
         get = get,
         setInverse = setInverse,
         getInverse = getInverse)
}

ResuelveMemoria <- function(x, ...){
    inversa <- x$getInverse()
    if (!is.null(inversa)){
              message("obteniendo datos de memoria ram")
              return(inversa)
    }
    mat <- x$get()
    inversa <- solve(mat, ...)
    x$setInverse(inversa)
    inversa
}
